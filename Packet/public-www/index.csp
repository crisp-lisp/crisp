#require "~/lib/web.csp"
#require "~/lib/templating.csp"

(lambda (url verb post)
	(respond
		(render-template-file "index.html" (
			("title" . "Crisp/Packet Example - Basic Page") 
			("url" . url) 
			("verb" . verb)
			("post" . (if (eq post nil) "No data." post)))) 200 "text/html"))